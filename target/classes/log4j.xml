<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE log4j:configuration SYSTEM "http://logging.apache.org/log4j/1.2/apidocs/org/apache/log4j/xml/doc-files/log4j.dtd">
<log4j:configuration xmlns:log4j="http://jakarta.apache.org/log4j/">
	<appender name="STDOUT" class="org.apache.log4j.ConsoleAppender">
		<layout class="org.apache.log4j.PatternLayout">
			<param name="ConversionPattern" value="[%d{{yyyy-MM-dd HH:mm:ss,SSS\} %-5p] [%t] %c{2\} - %m%n" />
		</layout>
	</appender>
	<!-- <appender name="FILE" class="org.apache.log4j.DailyRollingFileAppender"> 
		<param name="File" value="huadu.log" /> <layout class="org.apache.log4j.PatternLayout"> 
		<param name="ConversionPattern" value="[%d{{yyyy-MM-dd HH:mm:ss,SSS\} %-5p] 
		[%t] %c{2\} - %m%n" /> </layout> </appender> -->
	<logger name="org.hibernate.type" >
		<level value="error" />
	</logger>
	<logger name="com.synnex">
		<level value="error" />
	</logger>
	<root>
		<level value="error" />
		<appender-ref ref="STDOUT" />
		<appender-ref ref="FILE" />
	</root>
</log4j:configuration>